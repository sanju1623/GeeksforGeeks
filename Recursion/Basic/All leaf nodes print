/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode(int x) { val = x; }
 * }
 */
class Solution {
    
    public void check(TreeNode root){
        if(root.left==null && root.right==null)
          System.out.println(root.val);
        if(root.left!=null)
        check(root.left);
        if(root.right!=null)
        check(root.right);
        
    }
    
